{
    "$schema": "https://app.blockstudio.dev/schema",
    "apiVersion": 2,
    "name": "otw/outstream-player",
    "title": "Outstream Player",
    "category": "blockstudio",
    "icon": "star-filled",
    "blockstudio": {
        "attributes": [
            {
                "label": "Type",
                "type": "select",
                "id": "player_type",
