                "allowNull": false,
                "default": "jwplayer",
                "fallback": "jwplayer"
            },
            {
                "label": "Id",
                "type": "select",
                "id": "player_id",
                "options": [
                    {
                        "label": "Fishing Report",
                        "value": "9NydjPBo"
                    },
                    {
                        "label": "OTW Bass",
                        "value": "AIMvNPBp"
                    },
                    {
                        "label": "Fishing Kayak Buyer's Guide",
                        "value": "mOEg9gMl"
                    }
                ],
                "allowNull": true
            }
        ]
    }
}