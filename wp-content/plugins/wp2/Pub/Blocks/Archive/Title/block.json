{
   "$schema": "https://app.blockstudio.dev/schema",
   "apiVersion": 2,
   "name": "onthewater/archive-title",
   "title": "Archive Title",
   "category": "blockstudio",
   "icon": "heading",
   "supports": {
      "align": ["full","wide","center","left","right"],
      "typography": {
         "fontSize": true,
         "lineHeight": true,
         "textAlign": true,
         "__experimentalFontFamily": true,
         "__experimentalFontWeight": true,
         "__experimentalLetterSpacing": true
      }
   },
   "blockstudio": {
      "attributes": [
