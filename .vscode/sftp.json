{
    "name": "src",
    "openSsh": false,
    "remoteExplorer": {
        "filesExclude": [
            "**/**",
            "!wp-config.php",
            "!wp-content",
            "!wp-content/debug.log",
            "!wp-content/plugins",
            "!wp-content/plugins/wp2s",
            "!wp-content/plugins/wp2s/**",
            "!wp-content/plugins/wp2",
            "!wp-content/plugins/wp2/**",
            "!wp-content/plugins/wp2-*",
            "!wp-content/plugins/wp2-*/**",
            "!wp-content/plugins/connect-*",
            "!wp-content/plugins/connect-*/**",
            "!wp-content/plugins/extend-*",
            "!wp-content/plugins/extend-*/**",
            "!wp-content/themes",
            "!wp-content/themes/wp2s",
            "!wp-content/themes/wp2s/**",
            "!wp-content/mu-plugins",
            "!wp-content/mu-plugins/wp2",
            "!wp-content/mu-plugins/wp2/**",
            "!wp-content/mu-plugins/wp2-*",
            "!wp-content/mu-plugins/wp2-*/**",
            "!wp-content/mu-plugins/wp2-*.php",
            "!wp-content/mu-plugins/wp2.php",
            "!wp-content/mu-plugins/wp2s",
            "!wp-content/mu-plugins/wp2s/**",
            "!wp-content/mu-plugins/wp2s-*",
            "!wp-content/mu-plugins/wp2s-*/**",
            "!wp-content/mu-plugins/wp2s.php",
            "**/_dist/**",
            "**/_dist"
        ]
    },
    "ignore": [
        "**/**",
        "!wp-content",
        "!wp-content/plugins",
        "!wp-content/plugins/wp2s",
        "!wp-content/plugins/wp2s/**",
        "!wp-content/plugins/wp2",
        "!wp-content/plugins/wp2/**",
        "!wp-content/plugins/wp2-*",
        "!wp-content/plugins/wp2-*/**",
        "!wp-content/plugins/connect-*",
        "!wp-content/plugins/connect-*/**",
        "!wp-content/plugins/extend-*",
        "!wp-content/plugins/extend-*/**",
        "!wp-content/themes",
        "!wp-content/themes/wp2s",
        "!wp-content/themes/wp2s/**",
        "!wp-content/mu-plugins",
        "!wp-content/mu-plugins/wp2",
        "!wp-content/mu-plugins/wp2/**",
        "!wp-content/mu-plugins/wp2-*",
        "!wp-content/mu-plugins/wp2-*/**",
        "!wp-content/mu-plugins/wp2-*.php",
        "!wp-content/mu-plugins/wp2.php",
        "!wp-content/mu-plugins/wp2s",
        "!wp-content/mu-plugins/wp2s/**",
        "!wp-content/mu-plugins/wp2s-*",
        "!wp-content/mu-plugins/wp2s-*/**",
        "!wp-content/mu-plugins/wp2s.php",
        "**/_dist/**",
        "**/_dist"
    ],
    "defaultProfile": "main",
    "profiles": {
        "main": {
            "remotePath": "files",
            "remote": "wp2s-main",
            "uploadOnSave": true,
            "useTempFile": true
        }
    }
}