{
    "name": "src",
    "remoteExplorer": {
        "filesExclude": [
            "**/**",
            "!wp-config.php",
            "!wp-content",
            "!wp-content/mu-plugins",
            "!wp-content/mu-plugins/wp2**",
            "!wp-content/mu-plugins/wp2*/**",
            "!wp-content/plugins",
            "!wp-content/plugins/wp2**",
            "!wp-content/plugins/wp2*/**",
            "!wp-content/plugins/extend**",
            "!wp-content/plugins/extend*/**",
            "!wp-content/themes",
            "!wp-content/themes/wp2s",
            "!wp-content/themes/wp2s/**"
        ]
    },
    "ignore": [
        "**/**",
        "!wp-content",
        "!wp-content/mu-plugins",
        "!wp-content/mu-plugins/wp2**",
        "!wp-content/mu-plugins/wp2*/**",
        "!wp-content/plugins",
        "!wp-content/plugins/wp2**",
        "!wp-content/plugins/wp2*/**",
        "!wp-content/plugins/extend**",
        "!wp-content/plugins/extend*/**",
        "!wp-content/themes",
        "!wp-content/themes/wp2s",
        "!wp-content/themes/wp2s/**"
    ],
    "defaultProfile": "main",
    "profiles": {
        "main": {
            "remotePath": "files",
            "remote": "wp2s-main",
            "uploadOnSave": true,
            "useTempFile": true
        }
    }
}